{
  "name": "acme",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "nodemon",
    "start": "ng serve --port=4200 --ssl --ssl-key=config/webserver/https/key.pem --ssl-cert=config/webserver/https/certificate.cer",
    "start:de": "ng serve --configuration=de --port=4200 --open=true --ssl --ssl-key=config/webserver/https/key.pem --ssl-cert=config/webserver/https/certificate.cer",
    "start:bs": "ts-node -P scripts/tsconfig.json scripts/browsersync",
    "start:sirv": "sirv dist/acme --dev --gzip --http2 --cert config/webserver/https/certificate.cer --key config/webserver/https/key.pem --single",
    "start:es": "es-dev-server --port=4200 --root-dir=dist/acme --http2 --open --watch",
    "start:nginx": "ts-node -P scripts/tsconfig.json scripts/nginx",
    "start:hs": "http-server --port 4200 --ssl --key config\\webserver\\https\\key.pem --cert config\\webserver\\https\\certificate.cer --proxy https://localhost:8080 -c-1 .... dist\\acme",
    "start:ORIG": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "browserslist": "npx browserslist",
    "clean": "ts-node -P scripts/tsconfig.json scripts/clean",
    "doc": "typedoc --verbose --options typedoc.json src",
    "eslint": "npx eslint src/**/*.ts --fix",
    "i18n": "ng xi18n --output-path=src/locale --out-file=messages.de.xlf",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "lighthouse:hs": "npx lighthouse https://www.hs-karlsruhe.de/home/ --view --output-path=temp/www.hs-karlsruhe.de.html",
    "lighthouse:hskampus": "npx lighthouse http://www.iwi.hs-karlsruhe.de/hskampus-web/ --view --output-path=temp/app.hskampus.de.html --chrome-flags='--disable-web-security'",
    "local-web-server": "npx ws",
    "prettier": "npx prettier --write src/**/*.ts src/**/*.html src/**/*.scss",
    "sonarqube": "ts-node --script-mode scripts/sonarqube",
    "presource-map-explorer": "npm run build:prod -- --source-map",
    "source-map-explorer": "npx source-map-explorer dist/acme/*-es2015*.js* --html source-map-explorer.html",
    "stylelint": "npx stylelint src/app/**/*.scss --fix --allow-empty-input",
    "webhint": "hint https://localhost:4200",
    "prewebpack-bundle-analyzer": "npm run build:prod -- --stats-json",
    "webpack-bundle-analyzer": "npx webpack-bundle-analyzer dist/acme/stats.json dist/acme",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "11.1.0-next.2",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular/animations": "~11.1.0-next.2",
    "@angular/cdk": "11.0.2",
    "@angular/common": "~11.1.0-next.2",
    "@angular/compiler": "~11.1.0-next.2",
    "@angular/core": "~11.1.0-next.2",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "~11.1.0-next.2",
    "@angular/localize": "9.1.12",
    "@angular/material": "11.0.2",
    "@angular/platform-browser": "~11.1.0-next.2",
    "@angular/platform-browser-dynamic": "~11.1.0-next.2",
    "@angular/router": "~11.1.0-next.2",
    "@angular/service-worker": "~11.1.0-next.2",
    "@swimlane/ngx-charts": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-unicorn": "25.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.0-next.2",
    "@angular/cli": "~11.1.0-next.2",
    "@angular/compiler-cli": "~11.1.0-next.2",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "eslint-plugin-deprecation": "1.2.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-rxjs": "2.1.5",
    "hint": "6.1.1",
    "inquirer": "7.3.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "leasot": "11.4.0",
    "lighthouse": "6.5.0",
    "local-web-server": "4.2.1",
    "lws-redirect": "2.1.2",
    "minimist": "1.2.5",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "slash": "3.0.0",
    "source-map-explorer": "2.5.1",
    "stylelint": "13.8.0",
    "stylelint-config-sass-guidelines": "7.1.0",
    "stylelint-config-standard": "20.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "4.2.0"
  }
}
